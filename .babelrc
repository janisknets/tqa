{
    "presets": [
      [
        "@babel/env",
        {
            "modules": false,
            "targets": {
                "browsers": [
                    "last 2 versions"
                ]
            }
        }
    ],
    "@babel/react"
    ],
    "plugins": [
      [
          "transform-class-properties", { "spec": true }
      ],
      [
          "import",
          {
              "libraryName": "antd",
              "style": "css"
          }
      ]
  ]
  }